[flake8]
exclude = 
    ./config/
    ./venv/
    node_modules,
    frontend/node_modules,
    frontend/dist,
    frontend/.vite,
    frontend/.turbo,
    frontend/.next,
    frontend/build,
    frontend/.cache,
    frontend/.env,
    __pycache__,
    tests/__pycache__,
    src/**/__pycache__,
    utils/__pycache__
    despatch_endp/
    email_endp/

max-line-length = 120